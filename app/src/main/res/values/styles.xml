    <style name="DatePickerTheme" parent="ThemeOverlay.MaterialComponents.MaterialCalendar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="shapeAppearanceSmallComponent">@style/ShapeAppearance.App.DatePicker.SmallComponent</item>
        <item name="shapeAppearanceMediumComponent">@style/ShapeAppearance.App.DatePicker.MediumComponent</item>
    </style>

    <style name="ShapeAppearance.App.DatePicker.SmallComponent" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">cut</item>
    </style>

    <style name="ShapeAppearance.App.DatePicker.MediumComponent" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">16dp</item>
    </style>

    <style name="TimePickerTheme" parent="ThemeOverlay.MaterialComponents.TimePicker">
        <item name="colorPrimary">@color/colorPrimary</item>
